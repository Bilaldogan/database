{
  "uptimeConfig": {
    "enabled": true,
    "rules": [
      {
        "uriPattern": "(https?:\\/\\/)?([a-zA-Z]{2,63})*\\/statusCodes\\/401",
        "retryCount": 3,
        "retryInterval": 200
      },
      {
        "uriPattern": "(https?:\\/\\/)?([a-zA-Z]{2,63})*\\/statusCodes\\/402",
        "retryCount": 2,
        "retryInterval": 100
      },
      {
        "uriPattern": "(https?:\\/\\/)?([a-zA-Z]{2,63})*\\/statusCodes\\/404",
        "retryCount": 5,
        "retryInterval": 2000000000
      }
    ]
  }
}
